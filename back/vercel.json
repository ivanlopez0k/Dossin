{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
        {"src": "/viajes", "dest": "./index.js","methods":["GET","POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers":{
            "Access-Control-Allow-Origin": "*"
        }},
        {"src": "/contacto-cliente", "dest": "./index.js","methods":["GET","POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers":{
            "Access-Control-Allow-Origin": "*"
        }},
        {"src": "/contacto-proveedor", "dest": "./index.js","methods":["GET","POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers":{
            "Access-Control-Allow-Origin": "*"
        }},
        {"src": "/cv", "dest": "./index.js","methods":["GET","POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers":{
            "Access-Control-Allow-Origin": "*"
        }},
        {"src": "/(.*)", "dest": "./index.js","methods":["GET","POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers":{
            "Access-Control-Allow-Origin": "*"
        }}
    ]
  }